
<div class="internship-checklist internship-card internship_edit_button" id="internship-checklist-<%= checklist.id %>">
	<% if checklist.checked %>
		<i class="far fa-check-square"></i>
	<% else %>
		<i class="far fa-square"></i>
	<% end %>
	<p class="internship-checklist-name">
		<%= checklist.name %>
	</p>
</div>

<% Rails.application.routes.router.recognize(request) do |route, matches, param|%>
  <% page_name = route.name %>
<% end %>
